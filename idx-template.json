{
  "name": "Astro",
  "description": "The React Framework for the Web",
  "icon_image_url": "https://www.gstatic.com/monospace/240513/logo_firebase.svg",
  "params": [
    {
      "id": "language",
      "name": "Language",
      "type": "enum",
      "default": "ts",
      "options": {
        "js": "JavaScript",
        "ts": "TypeScript"
      },
      "required": true
    },
    {
      "id": "packageManager",
      "name": "Package Manager / Runtime",
      "type": "enum",
      "default": "npm",
      "options": {
        "npm": "npm",
        "yarn": "yarn",
        "bun": "bun"
      },
      "required": true
    },
    {
      "id": "srcDir",
      "name": "Use a src/ dir",
      "type": "boolean",
      "default": true,
      "required": false
    },
    {
      "id": "tailwind",
      "name": "Use Tailwind",
      "type": "boolean",
      "default": true,
      "required": false
    },
    {
      "id": "apikey",
      "name": "API KEY",
      "type": "text",
      "required": true
    }
  ]
}
