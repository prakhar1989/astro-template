{
  "name": "Astro",
  "description": "The React Framework for the Web",
  "categories": ["Web app"],
  "icon": "monicon-react",
  "iconImageUrl": "http://bar.svg",
  "params": [
    {
      "id": "language",
      "name": "Language",
      "type": "enum",
      "default": "ts",
      "options": {
        "js": "JavaScript",
        "ts": "TypeScript"
      },
      "required": true
    },
    {
      "id": "packageManager",
      "name": "Package Manager / Runtime",
      "type": "enum",
      "default": "npm",
      "options": {
        "npm": "npm",
        "yarn": "yarn",
        "bun": "bun"
      },
      "required": true
    },
    {
      "id": "srcDir",
      "name": "Use a src/ dir",
      "type": "boolean",
      "default": true,
      "required": false
    },
    {
      "id": "tailwind",
      "name": "Use Tailwind",
      "type": "boolean",
      "default": true,
      "required": false
    }
